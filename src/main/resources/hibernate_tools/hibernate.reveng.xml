<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="LONGVARCHAR" hibernate-type="text"
			not-null="false">
		</sql-type>
		<sql-type jdbc-type="INTEGER" hibernate-type="Integer"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="BIGINT" hibernate-type="Long" not-null="true">
		</sql-type>
	</type-mapping>

	 <!-- CP Database Tables mapping -->
	<table-filter match-name="person" match-catalog="persuasive_computation"></table-filter>
	<table-filter match-name="country" match-catalog="persuasive_computation"></table-filter>
	<table-filter match-name="influence_principle" match-catalog="persuasive_computation"></table-filter>
	<table-filter match-name="historic" match-catalog="persuasive_computation"></table-filter>
    <table-filter match-name="gender" match-catalog="persuasive_computation"></table-filter>
    <table-filter match-name="users" match-catalog="persuasive_computation"></table-filter>
</hibernate-reverse-engineering>
